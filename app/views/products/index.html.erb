<h1>Rainforest</h1>
<h2>All Products Page</h2>
<%= link_to "Add New Product", "products/new"%>
<% @products.each do |product| %>
  <h3><%= link_to "#{product.name}", "/products/#{product.id}" %></h3>
  <p><%= link_to "#{product.description}", "/products/#{product.id}" %></p>
  <p><%= product.price_in_cents %></p>
<% end %>
